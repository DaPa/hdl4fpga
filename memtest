rm pp pp1
SIZE=${1:-128};
SIZE=${1:-64};
#SIZE=${1:-32};
BLOCKS=${2:-262144};
BLOCKS=${2:-131072};
#BLOCKS=${2:-65536};
BLOCKS=${2:-1};
TIMEFORMAT="$i %lR"
./scope $BLOCKS $SIZE > pp
./scope $BLOCKS $SIZE > pp1
i=0
while cmp pp pp1 > log ; do 
	i=`expr $i + 1`
	echo -n $i ; time ./scope2 $BLOCKS $SIZE > pp1
done
cat log |(read g f e d c b a ; cat log ; echo '' ; tail -n +$a pp|head; echo '-----'; tail -n +$a pp1|head)
