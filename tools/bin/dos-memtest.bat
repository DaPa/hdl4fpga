@ECHO OFF
FOR %%F IN (%0) DO SET DIRNAME=%%~DPF
DEL /Q /F MASTER.DAT DUMP.DAT
"%DIRNAME%SCOPE" -p %1 -d %2 -h %3 > MASTER.DAT
ECHO MEMORY DUMPED
ECHO CHECKING LFSR DATA
"%DIRNAME%CHECK" -d %2 < MASTER.DAT
IF %ERRORLEVEL% NEQ 0 (
	ECHO UNSUCCESSFUL CHECK
	EXIT /B 1
)
ECHO MASTER DUMP OK
SET I=1
:NEXT
	"%DIRNAME%SCOPE" -p %1 -d %2 -h %3 > DUMP.DAT 2>NUL
	CMP MASTER.DAT DUMP.DAT
	IF %ERRORLEVEL% EQU 0 (
		ECHO DUMP %I% OK
		SET /A I=%I%+1
		GOTO NEXT
	)
